import random
caracteres = "+-/*!&$#?=@abcdefghijklnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ1234567890"
contra_longitud = int(input("¿Cúantos caracteres quieres en tu contraseña: "))
contra_generada = ""

for i in range(contra_longitud):
    contra_generada = contra_generada + random.choice(caracteres)

print("La contraseña generada es:", contra_generada)
